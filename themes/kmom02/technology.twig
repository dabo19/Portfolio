{% include 'incl/header.twig' %}

<body>
    {% include 'incl/nav.twig' %}

    <div class="main" role="main">

        <div class="singletech">

            <div class="techtitle">
                <h1>{{ meta.title }}</h1>
            </div>

            <div class="techlinks">
                {% for page in pages("technology") %}
                <div class="post">
                <h2><a href="{{ page.url }}">{{ page.title }}</a></h2>
                </div>
                {% endfor %}
            </div>

            <div class="techcontent">
                {{ content }}
                <br><a href="{{ "technology/index"|link }}">Tillbaka till Technologies</a>
            </div>
        
        </div>
        
    </div>
    
    {% include 'incl/footer.twig' %}
</body>

</html>
